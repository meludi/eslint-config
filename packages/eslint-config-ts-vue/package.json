{
  "name": "@meludi/eslint-config-ts-vue",
  "version": "0.1.4",
  "description": "Shareable configuration for vue typescript eslint",
  "private": false,
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "typescript-config",
    "vue-config"
  ],
  "author": {
    "email": "milad@al-ani.de",
    "name": "Milad Al-Ani"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./vue2": "./vue2.js",
    "./vue3": "./vue3.js",
    "./rules/vue2": "./rules/vue2.js",
    "./rules/vue3": "./rules/vue3.js",
    "./patch/modern-module-resolution": "./patch/modern-module-resolution.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "base.js",
    "vue2.js",
    "vue3.js",
    "patch",
    "rules"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precommit": "lint-staged",
    "unused": "eslint-find-rules --unused --no-error ./index.js",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"Not specified\" && exit 0"
  },
  "dependencies": {
    "@meludi/eslint-config-base": "^0.1.4",
    "@meludi/eslint-config-ts-base": "^0.1.3",
    "@meludi/eslint-config-vue": "^0.1.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.9.0",
    "vue-eslint-parser": "^9.0.0"
  },
  "peerDependencies": {
    "@meludi/eslint-config-base": "*",
    "@meludi/eslint-config-ts-base": "*",
    "@meludi/eslint-config-vue": "*",
    "eslint": ">= 8",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@meludi/eslint-config-prettier": "^0.1.4",
    "eslint": "^8.32.0",
    "eslint-find-rules": "^4.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "^16.x || ^18.x"
  },
  "bugs": {
    "url": "https://github.com/meludi/eslint-config/issues"
  },
  "homepage": "https://github.com/meludi/eslint-config/tree/main/packages/eslint-config-ts-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/meludi/eslint-config"
  }
}
