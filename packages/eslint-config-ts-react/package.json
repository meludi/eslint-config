{
  "name": "@meludi/eslint-config-ts-react",
  "version": "0.2.0",
  "description": "Shareable configuration for react typescript eslint",
  "private": false,
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "typescript-config",
    "react-config"
  ],
  "author": {
    "email": "milad@al-ani.de",
    "name": "Milad Al-Ani"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./react": "./react.js",
    "./hooks": "./hooks.js",
    "./a11y": "./a11y.js",
    "./rules/react": "./rules/react.js",
    "./patch/modern-module-resolution": "./patch/modern-module-resolution.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "base.js",
    "react.js",
    "hooks.js",
    "a11y.js",
    "patch",
    "rules"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "unused": "eslint-find-rules --unused --no-error ./index.js",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"Not specified\" && exit 0"
  },
  "dependencies": {
    "@meludi/eslint-config-base": "^0.2.0",
    "@meludi/eslint-config-react": "^0.2.0",
    "@meludi/eslint-config-ts-base": "^0.2.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "peerDependencies": {
    "@meludi/eslint-config-base": "*",
    "@meludi/eslint-config-react": "*",
    "@meludi/eslint-config-ts-base": "*",
    "eslint": ">= 8",
    "typescript": ">= 5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": "^16.x || ^18.x"
  },
  "bugs": {
    "url": "https://github.com/meludi/eslint-config/issues"
  },
  "homepage": "https://github.com/meludi/eslint-config/tree/main/packages/eslint-config-ts-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/meludi/eslint-config"
  }
}
