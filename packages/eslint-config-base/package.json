{
  "name": "@meludi/eslint-config-base",
  "version": "0.0.0",
  "description": "Shareable configuration for javascript eslint",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "author": {
    "email": "milad@al-ani.de",
    "name": "Milad Al-Ani"
  },
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./rules/base": "./rules/base.js",
    "./patch/modern-module-resolution": "./patch/modern-module-resolution.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "base.js",
    "patch",
    "rules"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "unused": "eslint-find-rules --unused ./index.js",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"OK - @meludi/eslint-config-base\" && exit 0"
  },
  "dependencies": {
    "@meludi/eslint-config-prettier": "*",
    "@rushstack/eslint-patch": "^1.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.27.5"
  },
  "peerDependencies": {
    "eslint": ">= 8",
    "prettier": ">= 2"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-find-rules": "^4.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "engines": {
    "node": "^16.x || ^18.x"
  },
  "bugs": {
    "url": "https://github.com/meludi/eslint-config/issues"
  },
  "homepage": "https://github.com/meludi/eslint-config/tree/main/packages/eslint-config-base",
  "repository": {
    "type": "git",
    "url": "https://github.com/meludi/eslint-config"
  }
}
